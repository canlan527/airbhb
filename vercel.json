{
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" },
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "settings": {
    "zeroConfig": true
  },
  "env": {
    "VERCEL_BUILDER": "pnpm",
    "VERCEL_BUILD_COMMAND": "pnpm run build"
  },
  "rewrites": [
    {
      "source": "/api",
      "destination": "http://codercba.com:1888/airbnb/api"
    }
  ]
}